    "react": "16.8.0-alpha.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/lodash.omit": "^4.5.4",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "react": "^16.13.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2"
  }
